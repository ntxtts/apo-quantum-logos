# Aria Quantum Interpreter Configuration

# Azure Function Settings
FUNCTIONS_WORKER_RUNTIME=python
FUNCTIONS_EXTENSION_VERSION=~4
PYTHON_VERSION=3.11

# Aria Quantum Settings
ARIA_QUANTUM_MODE=development
ARIA_MAX_QUBITS=32
ARIA_DEFAULT_BACKEND=simulator
ARIA_ENABLE_LOGGING=true
ARIA_LOG_LEVEL=INFO

# Quantum Computing Backends
ARIA_ENABLE_QISKIT=true
ARIA_ENABLE_CIRQ=true
ARIA_ENABLE_PENNYLANE=true

# Azure Services
AZURE_STORAGE_ACCOUNT=ariaquantumstorage
AZURE_COSMOS_DB_NAME=aria-quantum-db
AZURE_SERVICE_BUS_NAMESPACE=aria-quantum-messaging

# Security Settings
ARIA_API_KEY_REQUIRED=false
ARIA_CORS_ENABLED=true
ARIA_RATE_LIMITING=true

# Performance Settings
ARIA_CACHE_ENABLED=true
ARIA_CACHE_TTL=3600
ARIA_MAX_CONCURRENT_JOBS=10

# Monitoring and Telemetry
ARIA_ENABLE_TELEMETRY=true
ARIA_METRICS_ENDPOINT=https://aria-quantum-metrics.azurewebsites.net
ARIA_ALERT_EMAIL=admin@aria-quantum.com
